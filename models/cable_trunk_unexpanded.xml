<mujoco model="cable_trunk_model_example">
  <extension>
    <plugin plugin="mujoco.elasticity.cable"/>
  </extension>

  <asset>
      <!-- Optional material and texture assets if needed -->
      <texture name="grid" type="2d" builtin="checker" rgb1=".1 .2 .3" rgb2=".2 .3 .4" width="300" height="300"/>
      <material name="grid" texture="grid" texrepeat="8 8" reflectance=".2"/>
  </asset>

  <worldbody>
    <light pos="0 0 2.0"/> 
    <!-- Ground plane -->
    <geom size="10 10 .01" type="plane" material="grid"/>
    <composite prefix="actuated" type="cable" 
                vertex="0.00 0.00 0.70 0.00 0.00 0.69 0.00 0.00 0.67 0.00 0.00 0.66 0.00 0.00 0.65 0.00 0.00 0.64 0.00 0.00 0.62 0.00 0.00 0.61 0.00 0.00 0.60 0.00 0.00 0.58 0.00 0.00 0.57 0.00 0.00 0.56 0.00 0.00 0.55 0.00 0.00 0.53 0.00 0.00 0.52 0.00 0.00 0.51 0.00 0.00 0.49 0.00 0.00 0.48 0.00 0.00 0.47 0.00 0.00 0.46 0.00 0.00 0.44 0.00 0.00 0.43 0.00 0.00 0.42 0.00 0.00 0.41 0.00 0.00 0.39 0.00 0.00 0.38 0.00 0.00 0.37 0.00 0.00 0.35 0.00 0.00 0.34 0.00 0.00 0.33 0.00 0.00 0.32 0.00 0.00 0.30 0.00 0.00 0.29 0.00 0.00 0.28 0.00 0.00 0.26 0.00 0.00 0.25 0.00 0.00 0.24 0.00 0.00 0.23 0.00 0.00 0.21 0.00 0.00 0.20" 
                size="0.2 .1 6" initial="none">
        <plugin plugin="mujoco.elasticity.cable">
          <!--Units are in Pa (SI)-->
          <config key="twist" value="1e8"/>
          <config key="bend" value="1e8"/>
          <config key="vmax" value="0"/>
        </plugin>
        <joint kind="main" damping="0.25" armature="0.01"/>
        <geom type="capsule" size=".005" rgba=".8 .2 .1 1"/>
    </composite>
  </worldbody>

