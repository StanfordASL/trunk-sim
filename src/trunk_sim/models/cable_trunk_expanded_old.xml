<mujoco model="cable_trunk_model_example">
<option gravity="0 0 0"/>
  <compiler angle="radian"/>
  <extension>
    <plugin plugin="mujoco.elasticity.cable">
      <instance name="compositeactuated">
        <config key="twist" value="1e8"/>
        <config key="bend" value="1e8"/>
        <config key="vmax" value="0"/>
      </instance>
    </plugin>
  </extension>
  <custom>
    <text name="composite_actuated" data="rope_actuated"/>
  </custom>
  <asset>
    <texture type="2d" name="grid" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300"/>
    <material name="grid" texture="grid" texrepeat="8 8" reflectance="0.2"/>
  </asset>
  <worldbody>
    <geom size="10 10 0.01" type="plane" material="grid"/>
    <light pos="0 0 2" dir="0 0 -1"/>
    <body name="actuatedB_first" pos="0 0 0.7" quat="0 -0.707107 0 0.707107">
      <!-- Add slider joint with stiffness and damping to simulate elongation of material-->
      <joint name="slider_cable_world" pos="0 0 0" type="slide" axis="1 0 0" group="3" stiffness="1e2" damping="0.8"/>
      <geom name="actuatedG0" size="0.005 0.005" pos="0.005 0 0" quat="0.707107 0 -0.707107 0" type="capsule" rgba="0.8 0.2 0.1 1"/>
      <site name="actuatedS_first" pos="0 0 0" group="3"/> 
      <!-- Add sides for tendon to pass through-->
      <site name="cable_0_y_front" pos="0 0.005 0"/> 
      <site name="cable_0_y_back" pos="0 -0.005 0"/>
      <site name="cable_0_z_front" pos="0 0 0.005"/> 
      <site name="cable_0_z_back" pos="0 0 -0.005"/>
      <plugin instance="compositeactuated"/>
      <body name="actuatedB_1" pos="0.00999999 0 0">
        <joint name="actuatedJ_1" pos="0 0 0" type="ball" group="3" actuatorfrclimited="false" armature="0.01" damping="0.25"/>
        <geom name="actuatedG1" size="0.005 0.00999999" pos="0.00999999 0 0" quat="0.707107 0 -0.707107 0" type="capsule" rgba="0.8 0.2 0.1 1"/>
        <!-- Add sides for tendon to pass through-->
        <site name="cable_1_y_front" pos="0 0.005 0"/> 
        <site name="cable_1_y_back" pos="0 -0.005 0"/>
        <site name="cable_1_z_front" pos="0 0 0.005"/> 
        <site name="cable_1_z_back" pos="0 0 -0.005"/> 
        <plugin instance="compositeactuated"/>
        <body name="actuatedB_2" pos="0.02 0 0">
          <joint name="actuatedJ_2" pos="0 0 0" type="ball" group="3" actuatorfrclimited="false" armature="0.01" damping="0.25"/>
          <geom name="actuatedG2" size="0.005 0.005" pos="0.005 0 0" quat="0.707107 0 -0.707107 0" type="capsule" rgba="0.8 0.2 0.1 1"/>
          <!-- Add sides for tendon to pass through-->
          <site name="cable_2_y_front" pos="0 0.005 0"/> 
          <site name="cable_2_y_back" pos="0 -0.005 0"/>
          <site name="cable_2_z_front" pos="0 0 0.005"/> 
          <site name="cable_2_z_back" pos="0 0 -0.005"/>
          <plugin instance="compositeactuated"/>
        </body>
      </body>
    </body>
  </worldbody>
  <!-- add tendons attached to trunk-->
  <contact>
    <exclude body1="actuatedB_first" body2="actuatedB_1"/>
    <exclude body1="actuatedB_1" body2="actuatedB_2"/>
  </contact>
</mujoco>